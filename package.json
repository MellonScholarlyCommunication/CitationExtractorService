{
  "name": "oai-bridge-orchestrator",
  "version": "0.0.1",
  "description": "A OAI-PMH to Event Notifications orchestrator",
  "lsd:module": true,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "components",
    "dist/**/*.d.ts",
    "dist/**/*.js",
    "dist/**/*.js.map"
  ],
  "scripts": {
    "build": "npm run build:ts && npm run build:components",
    "build:ts": "npm exec tsc",
    "build:components": "componentsjs-generator -s dist -r oaior83",
    "prepare": "npm run build",
    "watch": "./bin/watch.sh",
    "extract": "npx orch --info --keep --in pre --out in rules/extractCitations.n3",
    "extract:pol": "npx pol --info --keep --in in --out out"
  },
  "keywords": [
    "oai-pmh",
    "event",
    "notifications",
    "ldn"
  ],
  "author": "Patrick Hochstenbach <Patrick.Hochstenbach@UGent.be>",
  "license": "MIT",
  "devDependencies": {
    "@types/jsonld": "^1.5.8",
    "@types/n3": "^1.10.4",
    "componentsjs-generator": "^3.1.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@xmldom/xmldom": "^0.8.7",
    "componentsjs": "^5.3.2",
    "koreografeye": "^0.2.5",
    "n3": "^1.16.4",
    "solid-bashlib": "^0.3.4",
    "xmldom": "^0.6.0",
    "xpath": "^0.0.32"
  }
}
